{
  "name": "coral-react",
  "version": "0.1.0",
  "private": true,
  "author": "Robot Coral",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robotcoral/coral-react.git"
  },
  "bugs": {
    "url": "https://github.com/robotcoral/coral-react/issues"
  },
  "homepage": "https://github.com/robotcoral/coral-react#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-dropdown-menu": "1.0.0",
    "@radix-ui/react-icons": "1.1.1",
    "@radix-ui/react-navigation-menu": "1.0.0",
    "@stitches/react": "1.2.8",
    "framer-motion": "6.5.1",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.5",
    "usehooks-ts": "2.6.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.2.4",
    "@svgr/webpack": "6.3.1",
    "@types/node": "18.0.5",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-formatjs": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "./*/**/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run eslint:fix"
    ],
    "*.{json,md}": [
      "npm run prettier:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --staged && lint-staged"
    }
  }
}
